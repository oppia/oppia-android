      answer_groups {
        outcome {
          dest_state_name: "Thinking in fractions Q3"
          feedback {
            html: "<p>No, that\'s not right. Start by drawing a picture of a cake divided into 8 equal pieces. Then cross out 3/8\302\240of the cake. What fraction of the cake is left?</p>"
            content_id: "feedback_3"
          }
