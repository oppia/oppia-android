    written_translations {
      key: "content"
      value {
        translation_mapping {
          key: "es"
          value {
            html: "<p>\"\302\241Te est\303\241s volviendo bueno en esto!\" dijo&nbsp;el Sr. Baker. \"Probemos solo uno m\303\241s.\"</p>\n<oppia-noninteractive-image alt-with-value=\"&amp;quot;Un cuadrado con unos partes coloreados en azul&amp;quot;\" caption-with-value=\"&amp;quot;&amp;quot;\" filepath-with-value=\"&amp;quot;img_20190816_183220_usf21b7zue_height_132_width_129.png&amp;quot;\"></oppia-noninteractive-image>\n\n<p>\"\302\277Qu\303\251 fracci\303\263n de esta imagen es de color azul?\"</p>"
          }
        }
      }
    }
    written_translations {
      key: "feedback_4"
      value {
        translation_mapping {
