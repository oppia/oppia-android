    }
    written_translations {
      key: "content"
      value {
        translation_mapping {
          key: "hi"
          value {
            html: "<p>James ne apne baqi adla badli ratios nikalkar, apne doston ke saath khilaune badalna shuroo kar diya.&nbsp;</p>\n\n<p>\342\200\234Ham tumhein 50 kanche denge, aur tum hamko 20 strawberry dena,\342\200\235 James ne kaha apne ek dost se.&nbsp;</p>\n\n<p>Is maamle mein kanche se stawberry ka kya ratio hai?</p>"
          }
        }
      }
    }
    written_translations {
      key: "feedback_3"
      value {
        translation_mapping {
          key: "hi"
          value {
            html: "<p>Nahin, tumne ghalta order likha hai. Ratios mein, number ka arrangement sahi hona chahiye! Phir se koshish kariye.&nbsp;</p>"
          }
        }
      }
    }
    written_translations {
      key: "feedback_1"
      value {
        translation_mapping {
          key: "hi"
