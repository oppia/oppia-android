states {
  key: "Definition of ratio"
  value {
    name: "Definition of ratio"
    recorded_voiceovers {
      key: "content"
      value {
        voiceover_mapping {
          key: "hi"
          value {
            file_size_bytes: 1008285
            file_name: "content-hi-djt74p6bam.mp3"
