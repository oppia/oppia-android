    written_translations {
      key: "feedback_2"
      value {
        translation_mapping {
          key: "hi"
          value {
            html: "<p>Ratio sahi hai! Lekin, ab se, apna jawaab colon ke saath hi likhna. Phir se koshish kariye.</p>"
