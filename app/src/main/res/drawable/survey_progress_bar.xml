<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:id="@+id/background">
    <shape>
      <corners android:radius="0dp" />
      <solid android:color="@color/component_color_shared_progress_bar_background_color" />
      </shape>
  </item>
  <item android:id="@+id/progress">
    <scale android:scaleWidth="100%">
      <shape>
        <corners android:radius="0dp" />
        <solid android:color="@color/component_color_survey_progress_bar_solid_color" />
        <stroke
          android:width="1dp"
          android:color="@color/component_color_shared_white_background_color" />
      </shape>
    </scale>
  </item>
</layer-list>
